<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <title>Build Your Resume</title>
  </head>
  <body>
    <div class="container">
      <h1>Build a Resume!</h1>
      <a href="/new">Add A New Job</a>
      @foreach (var job in Model)
      {
      <h3>Job Title @job.GetJobTitle()</h3>
      <p>company Name: @job.GetCompanyName()</p>
      <p>Location: @job.GetLocation()</p>
      <p>Start Date: @job.GetStartDate()</p>
      <p>End Date @job.GetEndDate()</p>
      <p>Discription: @job.GetDescription()</p>
      }
    </div>

  </body>
</html>
